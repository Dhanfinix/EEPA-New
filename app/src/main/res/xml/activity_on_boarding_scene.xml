<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
        <OnSwipe motion:dragDirection="dragLeft" />
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="0dp"
            android:layout_height="300dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.194"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/gambar1"
            android:layout_marginStart="24dp"
            android:layout_marginEnd="24dp" />
        <Constraint
            android:id="@+id/menu1"
            motion:layout_constraintEnd_toStartOf="@id/menu2"
            android:layout_width="20dp"
            android:layout_height="20dp"
            motion:layout_constraintTop_toTopOf="@id/menu3">
            <CustomAttribute
                motion:attributeName="crossfade"
                motion:customFloatValue="1" />
        </Constraint>
        <Constraint
            android:id="@+id/menu2"
            motion:layout_constraintEnd_toStartOf="@id/menu3"
            android:layout_width="20dp"
            android:layout_height="20dp"
            motion:layout_constraintTop_toTopOf="@id/menu3">
            <CustomAttribute
                motion:attributeName="crossfade"
                motion:customFloatValue="0" />
        </Constraint>
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/gambar1"
            android:layout_width="0dp"
            android:layout_height="300dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.201"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintEnd_toStartOf="parent" />
        <Constraint
            android:id="@+id/judul1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="@id/gambar1"
            android:layout_marginTop="96dp"
            motion:layout_constraintTop_toBottomOf="@id/gambar1"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintEnd_toStartOf="parent" />
        <Constraint
            android:id="@+id/desc1"
            motion:layout_constraintEnd_toEndOf="@id/gambar1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@id/gambar1"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintStart_toStartOf="@id/gambar1"
            android:layout_marginTop="160dp" />
        <Constraint
            android:id="@+id/menu1"
            motion:layout_constraintEnd_toStartOf="@id/menu2"
            android:layout_width="20dp"
            android:layout_height="20dp"
            motion:layout_constraintTop_toTopOf="@id/menu3">
            <CustomAttribute
                motion:attributeName="crossfade"
                motion:customFloatValue="0" />
        </Constraint>
        <Constraint
            android:id="@+id/menu2"
            motion:layout_constraintEnd_toStartOf="@id/menu3"
            android:layout_width="20dp"
            android:layout_height="20dp"
            motion:layout_constraintTop_toTopOf="@id/menu3">
            <CustomAttribute
                motion:attributeName="crossfade"
                motion:customFloatValue="1" />
        </Constraint>
        <Constraint
            android:id="@+id/menu3"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="20dp"
            android:layout_height="20dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="20dp"
            motion:layout_constraintStart_toStartOf="parent">
            <CustomAttribute
                motion:attributeName="crossfade"
                motion:customFloatValue="0" />
        </Constraint>
        <Constraint
            android:id="@+id/gambar2"
            android:layout_width="0dp"
            android:layout_height="300dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.194"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/judul2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@id/gambar2"
            motion:layout_constraintStart_toStartOf="@id/gambar2"
            android:layout_marginTop="96dp"
            motion:layout_constraintEnd_toEndOf="@+id/gambar2" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_marginTop="160dp"
            android:layout_width="wrap_content"
            motion:layout_constraintTop_toBottomOf="@id/gambar2"
            motion:layout_constraintStart_toStartOf="@id/gambar2"
            android:id="@+id/desc2"
            motion:layout_constraintEnd_toEndOf="parent" />
    </ConstraintSet>
    <ConstraintSet
        android:id="@+id/onBoard3"
        motion:deriveConstraintsFrom="@+id/end" >
        <Constraint
            motion:layout_constraintVertical_bias="0.194"
            android:layout_height="300dp"
            motion:layout_constraintEnd_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="0dp"
            android:id="@+id/gambar2" />
        <Constraint
            android:id="@+id/desc2"
            motion:layout_constraintEnd_toEndOf="@id/gambar2"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            motion:layout_constraintTop_toBottomOf="@id/gambar2"
            motion:layout_constraintStart_toStartOf="@id/gambar2"
            android:layout_marginTop="160dp" />
        <Constraint
            android:id="@+id/menu2"
            motion:layout_constraintEnd_toStartOf="@id/menu3"
            android:layout_width="20dp"
            android:layout_height="20dp"
            motion:layout_constraintTop_toTopOf="@id/menu3"
            motion:crossfade="0" >
            <CustomAttribute
                motion:attributeName="crossfade"
                motion:customFloatValue="0" />
        </Constraint>
        <Constraint
            android:id="@+id/menu3"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="20dp"
            android:layout_height="20dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="20dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:crossfade="1" >
            <CustomAttribute
                motion:attributeName="crossfade"
                motion:customFloatValue="1" />
        </Constraint>
        <Constraint
            android:id="@+id/menu4"
            motion:layout_constraintStart_toEndOf="@id/menu3"
            android:layout_width="20dp"
            android:layout_height="20dp"
            motion:layout_constraintTop_toTopOf="@id/menu3">
            <CustomAttribute
                motion:attributeName="crossfade"
                motion:customFloatValue="0" />
        </Constraint>
        <Constraint
            motion:layout_constraintStart_toEndOf="@+id/gambar2"
            android:layout_width="0dp"
            android:layout_height="300dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.194"
            motion:layout_constraintTop_toTopOf="parent"
            android:id="@+id/gambar3"
            motion:layout_constraintEnd_toEndOf="parent" />
        <Constraint
            android:id="@+id/judul3"
            motion:layout_constraintEnd_toEndOf="@id/gambar3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@id/gambar3"
            motion:layout_constraintStart_toStartOf="@id/gambar3"
            android:layout_marginTop="96dp" />
        <Constraint
            android:id="@+id/desc3"
            motion:layout_constraintEnd_toEndOf="@id/gambar3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="@id/gambar3"
            android:layout_marginTop="160dp"
            motion:layout_constraintTop_toBottomOf="@id/gambar3"
            motion:layout_constraintHorizontal_bias="0.491" />
    </ConstraintSet>
    <Transition
        motion:constraintSetStart="@+id/end"
        motion:constraintSetEnd="@+id/onBoard3" >
        <OnSwipe motion:dragDirection="dragLeft" />
    </Transition>
    <ConstraintSet
        android:id="@+id/onBoard4"
        motion:deriveConstraintsFrom="@+id/onBoard3" >
        <Constraint
            motion:layout_constraintVertical_bias="0.194"
            android:layout_height="300dp"
            motion:layout_constraintEnd_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="0dp"
            android:id="@+id/gambar3" />
        <Constraint
            android:layout_height="300dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="0dp"
            motion:layout_constraintVertical_bias="0.194"
            android:id="@+id/gambar4"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp" />
        <Constraint
            android:id="@+id/judul4"
            motion:layout_constraintEnd_toEndOf="@id/gambar4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@id/gambar4"
            motion:layout_constraintStart_toStartOf="@id/gambar4"
            android:layout_marginTop="96dp" />
        <Constraint
            android:id="@+id/desc4"
            motion:layout_constraintEnd_toEndOf="@id/gambar4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="@id/gambar4"
            android:layout_marginTop="160dp"
            motion:layout_constraintTop_toBottomOf="@id/gambar4"
            motion:layout_constraintHorizontal_bias="0.492" />
        <Constraint
            android:id="@+id/menu3"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="20dp"
            android:layout_height="20dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="20dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:crossfade="1">
            <CustomAttribute
                motion:attributeName="crossfade"
                motion:customFloatValue="0" />
        </Constraint>
        <Constraint
            android:id="@+id/menu4"
            motion:layout_constraintStart_toEndOf="@id/menu3"
            android:layout_width="20dp"
            android:layout_height="20dp"
            motion:layout_constraintTop_toTopOf="@id/menu3"
            motion:crossfade="1" >
            <CustomAttribute
                motion:attributeName="crossfade"
                motion:customFloatValue="1" />
        </Constraint>
        <Constraint
            android:id="@+id/menu5"
            motion:layout_constraintStart_toEndOf="@id/menu4"
            android:layout_width="20dp"
            android:layout_height="20dp"
            motion:layout_constraintTop_toTopOf="@id/menu3">
            <CustomAttribute
                motion:attributeName="crossfade"
                motion:customFloatValue="0" />
        </Constraint>
    </ConstraintSet>
    <Transition
        motion:constraintSetStart="@+id/onBoard3"
        motion:constraintSetEnd="@+id/onBoard4" >
        <OnSwipe motion:dragDirection="dragLeft" />
    </Transition>
    <ConstraintSet
        android:id="@+id/onBoard5"
        motion:deriveConstraintsFrom="@+id/onBoard4" >
        <Constraint
            android:layout_height="300dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="0dp"
            motion:layout_constraintVertical_bias="0.194"
            android:id="@+id/gambar4"
            motion:layout_constraintEnd_toStartOf="parent" />
        <Constraint
            motion:layout_constraintStart_toEndOf="@+id/gambar1"
            android:layout_width="0dp"
            android:layout_height="300dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.194"
            motion:layout_constraintTop_toTopOf="parent"
            android:id="@+id/gambar5"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_marginStart="50dp" />
        <Constraint
            android:id="@+id/judul5"
            motion:layout_constraintEnd_toEndOf="@id/gambar5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@id/gambar5"
            motion:layout_constraintHorizontal_bias="0.497"
            motion:layout_constraintStart_toStartOf="@id/gambar5"
            android:layout_marginTop="96dp"
            android:layout_marginStart="-50dp" />
        <Constraint
            android:id="@+id/desc5"
            motion:layout_constraintEnd_toEndOf="@id/gambar5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="@id/gambar5"
            android:layout_marginTop="160dp"
            motion:layout_constraintTop_toBottomOf="@id/gambar5"
            android:layout_marginStart="-50dp" />
        <Constraint
            android:id="@+id/menu5"
            motion:layout_constraintStart_toEndOf="@id/menu4"
            android:layout_width="20dp"
            android:layout_height="20dp"
            motion:layout_constraintTop_toTopOf="@id/menu3"
            motion:crossfade="1" >
            <CustomAttribute
                motion:attributeName="crossfade"
                motion:customFloatValue="1" />
        </Constraint>
        <Constraint
            android:id="@+id/menu4"
            motion:layout_constraintStart_toEndOf="@id/menu3"
            android:layout_width="20dp"
            android:layout_height="20dp"
            motion:layout_constraintTop_toTopOf="@id/menu3"
            motion:crossfade="1">
            <CustomAttribute
                motion:attributeName="crossfade"
                motion:customFloatValue="0" />
        </Constraint>
    </ConstraintSet>
    <Transition
        motion:constraintSetStart="@+id/onBoard4"
        motion:constraintSetEnd="@+id/onBoard5" >
        <OnSwipe motion:dragDirection="dragLeft" />
    </Transition>
    <ConstraintSet
        android:id="@+id/login"
        motion:deriveConstraintsFrom="@+id/onBoard5" >
        <Constraint
            android:layout_height="300dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="0dp"
            motion:layout_constraintVertical_bias="0.194"
            android:id="@+id/gambar5"
            motion:layout_constraintEnd_toStartOf="parent" />
        <Constraint
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:id="@+id/bgLogin"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginBottom="0dp" />
        <Constraint
            android:layout_height="20dp"
            android:layout_width="20dp"
            android:id="@+id/menu3"
            motion:layout_editor_absoluteX="196dp"
            motion:layout_constraintTop_toBottomOf="@+id/bgLogin"
            motion:layout_editor_absoluteY="691dp" />
        <Constraint
            motion:layout_constraintEnd_toEndOf="@id/bgLogin"
            android:layout_width="150dp"
            android:layout_height="150dp"
            motion:layout_constraintTop_toBottomOf="@id/kenalan"
            motion:layout_constraintStart_toStartOf="@id/bgLogin"
            android:layout_marginTop="32dp"
            android:id="@+id/avatar" />
    </ConstraintSet>
    <Transition
        motion:constraintSetStart="@+id/onBoard5"
        motion:constraintSetEnd="@+id/login" >
        <OnSwipe motion:dragDirection="dragUp" />
        <OnClick motion:targetId="@id/btnMasuk" />
    </Transition>
</MotionScene>